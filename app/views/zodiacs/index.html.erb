<h1>List of Zodiac</h1>

<table class='table table-striped table-hover'>
  <tr>
    <th>Sign</th>
    <th>Creature</th>
    <th>Action</th>
  </tr>
  <% @list_of_zodiacs.each do |zodiac| %>
  <tr>
    <td><%= zodiac.sign.capitalize %></td>
    <td><%= zodiac.creature.capitalize %></td>
    <td><a href = "http://localhost:3000/delete_zodiac/<%= zodiac.id %>">Delete</a></td>
  </tr>
  <% end %>
</table>

